    }

    /* ibm-plex-mono-italic - latin */
    @font-face {
      font-family: 'IBM Plex Mono';
      font-style: italic;
      font-display: swap;
      font-weight: 400;
      src: url("{{asset 'fonts/ibm-plex-mono/ibm-plex-mono-v6-latin-italic.eot'}}");
      /* IE9 Compat Modes */
