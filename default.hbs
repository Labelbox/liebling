      /* Legacy iOS */
    }

    /* ibm-plex-mono-regular - latin */
    @font-face {
      font-family: 'IBM Plex Mono';
      font-style: normal;
      font-display: swap;
      font-weight: 400;
      src: url("{{asset 'fonts/ibm-plex-mono/ibm-plex-mono-v6-latin-regular.eot'}}");
      /* IE9 Compat Modes */
      src: local('IBM Plex Mono'),
      local('IBMPlexMono'),
      url("{{asset 'fonts/ibm-plex-mono/ibm-plex-mono-v6-latin-regular.eot?#iefix'}}") format('embedded-opentype'),
      /* IE6-IE8 */
      url("{{asset 'fonts/ibm-plex-mono/ibm-plex-mono-v6-latin-regular.woff2'}}") format('woff2'),
